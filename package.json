{
  "name": "ai-community",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=24.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "dev": "pnpm build && wrangler pages dev",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "NODE_ENV=production node dist/index.js",
    "preview": "vite preview --host",
    "check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "createdb": "wrangler d1 create ai-community",
    "select:local:msg": "wrangler d1 execute DB --local --command \"select user_id,content from messages limit 10;\"",
    "migrate:create": "wrangler d1 migrations create DB",
    "migrate:local": "wrangler d1 migrations apply DB --local",
    "migrate:remote": "wrangler d1 migrations apply DB --remote",
    "migrate:list": "wrangler d1 migrations list DB"
  },
